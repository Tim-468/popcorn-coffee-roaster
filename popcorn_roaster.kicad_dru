(version 1)
(
	rule "AC line clearance"
	(constraint clearance (min 2.5mm))
	(condition "A.hasNetclass('AC_Line') && A.NetName != B.NetName && !(A.NetName == '/Smart Roaster/triac_gate')")
)
(
	rule "AC line clearance exclusions"
	(constraint clearance (min 0.3mm))
	(condition "(A.NetName == '/Smart Roaster/heat_blue' && B.NetName == '/Smart Roaster/heat_black') || (A.NetName == '/Smart Roaster/triac_g') || (A.NetName == '/Smart Roaster/triac_a2')")
)
